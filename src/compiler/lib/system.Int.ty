function parse(str:string):int{
    var ret=0;
    var pos=1;
    for(var i=str.buffer.length-1 ; i>=0 ; i--){
        var ch=str[i];
        if(ch<'0'||ch>'9'){
            throw "无效字符"+new string( {[ str[i] ]} );
        }else{
            ret=ret + (int)(ch-'0') * pos;
        }
        pos=pos*10;
    }
    return ret;
};