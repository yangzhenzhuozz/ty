class thread{
    var task:()=>void;
    var _name:string;
    get name():string{
        return _name;
    };
    set name(v:string){
        _name=v;
    };
    thread(t:()=>void){
        this.task=t;
    }
    thread(){
    }
    function start(){
        if((object)task==null){
            throw "the task of thread is null";
        }else{
            system.fork(()=>{
                system.setCurrentThread(this);
                task();
            });
        }
    };
}